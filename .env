# Deployment
HOSTED_DOMAIN=abais.vercel.app

# Optional
API_HTTP_PROXY_URI= # Just leave it empty, kept here for some reason

# For internal api calls between serverless functions
INTERNAL_API_BASE_URL= # The index url of your deployment
INTERNAL_API_CALL_SECRET= # Secret generated by your own

# Generate
NEXT_PUBLIC_MAX_INPUT_CHARS=200
MAX_REQUEST_PER_IP_PER_DAY=20
MAX_GENERATE_TOKENS=500 # Increase this if your stories got truncated

# OpenAI
NEXT_PUBLIC_GPT_4_AVAILABLE= # true if you have access to GPT 4
OPENAI_API_KEY= # Your API key

# Hugging Face
HUGGING_FACE_API_KEY= # Your Hugging Face API key

# Database
MONGO_DB_URL= # MongoDB connect URL

# Amazon S3 / Cloudflare R2
NEXT_PUBLIC_S3_PUB_URL_PREFIX= # Base/Common url of your images on AWS S3/Cloudflare R2
S3_BUCKET_NAME=
S3_ENDPOINT=
S3_REGION=auto
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=